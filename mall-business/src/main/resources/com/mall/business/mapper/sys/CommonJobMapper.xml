<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mall.business.mapper.sys.CommonJobMapper">
    <insert id="insert" parameterType="com.mall.entity.sys.CommonJobDO" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO common_job
        <trim prefix="(" suffix=")" suffixOverrides=","> 
            <if test="id != null">id,</if>
            <if test="jobName != null and jobName != ''">job_name,</if>
            <if test="pauseStatus != null">pause_status,</if>
            <if test="beanName != null and beanName != ''">bean_name,</if>
            <if test="methodName != null and methodName != ''">method_name,</if>
            <if test="cronExpression != null and cronExpression != ''">cron_expression,</if>
            <if test="params != null and params != ''">params,</if>
            <if test="remark != null and remark != ''">remark,</if>
            <if test="createUserId != null">create_user_id,</if>
            <if test="createUserName != null and createUserName != ''">create_user_name,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateUserId != null">update_user_id,</if>
            <if test="updateUserName != null and updateUserName != ''">update_user_name,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="isDel != null">is_del,</if>
         </trim>
        <trim prefix="VALUES (" suffix=")" suffixOverrides=","> 
            <if test="id != null">#{id},</if>
            <if test="jobName != null and jobName != ''">#{jobName},</if>
            <if test="pauseStatus != null">#{pauseStatus},</if>
            <if test="beanName != null and beanName != ''">#{beanName},</if>
            <if test="methodName != null and methodName != ''">#{methodName},</if>
            <if test="cronExpression != null and cronExpression != ''">#{cronExpression},</if>
            <if test="params != null and params != ''">#{params},</if>
            <if test="remark != null and remark != ''">#{remark},</if>
            <if test="createUserId != null">#{createUserId},</if>
            <if test="createUserName != null and createUserName != ''">#{createUserName},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateUserId != null">#{updateUserId},</if>
            <if test="updateUserName != null and updateUserName != ''">#{updateUserName},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="isDel != null">#{isDel},</if>
         </trim>
    </insert>

    <update id="update" parameterType="com.mall.entity.sys.CommonJobDO">
        UPDATE common_job
        <set> 
            <if test="id != null"> id = #{id}, </if>
            <if test="jobName != null and jobName != ''"> job_name = #{jobName}, </if>
            <if test="pauseStatus != null"> pause_status = #{pauseStatus}, </if>
            <if test="beanName != null and beanName != ''"> bean_name = #{beanName}, </if>
            <if test="methodName != null and methodName != ''"> method_name = #{methodName}, </if>
            <if test="cronExpression != null and cronExpression != ''"> cron_expression = #{cronExpression}, </if>
            <if test="params != null and params != ''"> params = #{params}, </if>
            <if test="remark != null and remark != ''"> remark = #{remark}, </if>
            <if test="createUserId != null"> create_user_id = #{createUserId}, </if>
            <if test="createUserName != null and createUserName != ''"> create_user_name = #{createUserName}, </if>
            <if test="createTime != null"> create_time = #{createTime}, </if>
            <if test="updateUserId != null"> update_user_id = #{updateUserId}, </if>
            <if test="updateUserName != null and updateUserName != ''"> update_user_name = #{updateUserName}, </if>
            <if test="updateTime != null"> update_time = #{updateTime}, </if>
            <if test="isDel != null"> is_del = #{isDel}, </if>
         </set>
        WHERE id = #{id}
    </update>

    <delete id="delete" parameterType="long">
        DELETE FROM common_job
        WHERE id = #{id}
    </delete>

    <delete id="deleteByIds">
        DELETE FROM common_job
        WHERE id IN
        <foreach collection="ids" item="id" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

    <select id="searchByCondition" parameterType="com.mall.dto.condition.sys.CommonJobConditionDTO"
    resultMap="CommonJobResultMap">
        SELECT
        <include refid="queryColumn"/>
        FROM common_job
        <include refid="queryWhere"/>
    </select>

    <resultMap type="com.mall.entity.sys.CommonJobDO" id="CommonJobResultMap">
        <result property="id" column="id"/>
        <result property="jobName" column="job_name"/>
        <result property="pauseStatus" column="pause_status"/>
        <result property="beanName" column="bean_name"/>
        <result property="methodName" column="method_name"/>
        <result property="cronExpression" column="cron_expression"/>
        <result property="params" column="params"/>
        <result property="remark" column="remark"/>
        <result property="createUserId" column="create_user_id"/>
        <result property="createUserName" column="create_user_name"/>
        <result property="createTime" column="create_time"/>
        <result property="updateUserId" column="update_user_id"/>
        <result property="updateUserName" column="update_user_name"/>
        <result property="updateTime" column="update_time"/>
        <result property="isDel" column="is_del"/>
     </resultMap>

    <sql id="queryColumn"> 
