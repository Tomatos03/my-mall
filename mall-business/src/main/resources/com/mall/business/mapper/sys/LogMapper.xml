<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mall.business.mapper.sys.LogMapper">
    <resultMap type="com.mall.entity.sys.LogDO" id="LogResult">
        <result property="id" column="id"/>
        <result property="createUserId" column="create_user_id"/>
        <result property="createUserName" column="create_user_name"/>
        <result property="createTime" column="create_time"/>
        <result property="updateUserId" column="update_user_id"/>
        <result property="updateUserName" column="update_user_name"/>
        <result property="updateTime" column="update_time"/>
        <result property="isDel" column="is_del"/>
        <result property="methodName" column="method_name"/>
        <result property="description" column="description"/>
        <result property="requestIp" column="request_ip"/>
        <result property="browser" column="browser"/>
        <result property="url" column="url"/>
        <result property="param" column="param"/>
        <result property="time" column="time"/>
        <result property="exception" column="exception"/>
        <result property="status" column="status"/>
    </resultMap>

    <sql id="selectLogColumn">
        id,
        create_user_id,
        create_user_name,
        create_time,
        update_user_id,
        update_user_name,
        update_time,
        is_del,
        method_name,
        description,
        request_ip,
        browser,
        url,
        param,
        time,
        exception,
        status
    </sql>

    <insert id="insert" parameterType="com.mall.entity.sys.LogDO">
        INSERT INTO biz_log
        <trim prefix="(" suffix=")" suffixOverrides=",">
            method_name,
            description,
            request_ip,
            url,
            time,
            status,
            create_user_id,
            create_user_name,
            <if test="browser != null">browser,</if>
            <if test="param != null">param,</if>
            <if test="exception != null">exception,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateUserId != null">update_user_id,</if>
            <if test="updateUserName != null">update_user_name,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="isDel != null">is_del,</if>
        </trim>
        VALUES
        <trim prefix="(" suffix=")" suffixOverrides=",">
            #{methodName},
            #{description},
            #{requestIp},
            #{url},
            #{time},
            #{status},
            #{createUserId},
            #{createUserName},
            <if test="browser != null">#{browser},</if>
            <if test="param != null">#{param},</if>
            <if test="exception != null">#{exception},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateUserId != null">#{updateUserId},</if>
            <if test="updateUserName != null">#{updateUserName},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="isDel != null">#{isDel},</if>
        </trim>
    </insert>
</mapper>